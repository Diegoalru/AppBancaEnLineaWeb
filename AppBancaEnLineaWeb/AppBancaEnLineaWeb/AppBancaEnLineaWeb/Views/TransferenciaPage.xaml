             x:Class="AppBancaEnLineaWeb.Views.TransferenciaPage"
             Title="Transferencias"
             Icon="">
    <ContentPage.Content>
        <StackLayout Padding="30" VerticalOptions="Center">
            <Label 
                Text="Transferencias" 
				HorizontalOptions="Center" 
				TextColor="#000000"
				FontSize="Medium"
				TextDecorations="Underline"
				FontAttributes="Bold" 
                Margin="40">
            </Label>
            <!--MAIN-->
            <!--END MAIN-->
            <!--FOOTER-->
            <Button
                Text="Ver Lista de Transferencias"
                Clicked="Btn_ListaTransferencias"
                ></Button>
            <!--END FOOTER-->
        </StackLayout>
    </ContentPage.Content>
</ContentPage>